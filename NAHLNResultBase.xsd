			<!-- Tag, Wing  -->
			<xsd:enumeration value="TAT"/>
			<!-- Tattoo -->
			<xsd:enumeration value="TATB"/>
			<!-- Tattoo, Body  -->
			<xsd:enumeration value="TATE"/>
			<!-- Tattoo, Ear  -->
			<xsd:enumeration value="TATL"/>
			<!-- Tattoo, Lip  -->
			<xsd:enumeration value="U"/>
			<!-- Unspecified identifier -->
			<xsd:enumeration value="USAIN"/>
