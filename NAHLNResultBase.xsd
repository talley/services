<?xml version="1.0" encoding="UTF-8"?>
<!-- ******* Version 1_0_9_1 of the NAHLN Base Result Schema ******* -->
<!--
This is the basic schema for all NAHLN laboratory results messages.  
It includes all fields that we anticipate needing in any of the disease 
program or surveillance testing.  It requires only the minimum needed 
for all types of testing.	
-->

 <!-- 10/06/2010 - 1_0_9_0 Changes include:
 Various backward compatible changes found in developing SIV message examples.
 Relaxed a number of constraints.  
 May expand enumeration of Reason for Testing (TBD) 
  
 4/26/2013 1_0_9_1 Removed incorrect length restrictions.
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:hl7="urn:hl7-org:v2xml">
	<xsd:simpleType name="OID">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\s)*(([1-9][0-9]*)|(0))(\.(([1-9][0-9]*)|(0)))+(\s)*"></xsd:pattern>
			<!--<xsd:pattern value="(\s)*([1-9][0-9]*)(\.([1-9][0-9]*))+(\s)*"></xsd:pattern>--> <!-- ** Updated by SLINKE on 8/24/2010 to comply with CSF ELISA OID ** -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AREACODE">
		<!-- 11/30/2007 Slinke Added Expression to validate Area Code -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[2-9]{1}\d{2}"></xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PHONENUMBER">
		<!-- 11/30/2007 Slinke Added Expression to validate Phone Num -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="\d{7}"></xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EXTENSION">
		<!-- 11/30/2007 Slinke Added Expression to validate Extension -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="\d{1,5}"></xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ISO">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="ISO"></xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LN">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="LN"></xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DateTime">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\d{4}(((0[1-9])|(1[0-2]))(((0[1-9])|([1-2]\d)|(3[0-1]))((([01]\d|2[0-3])([0-5]\d))(([0-5]\d)((\.\d{1,4}))?)?)?)?)?([+\-](([0]\d|1[0-3])([0-5]\d)))?)?"></xsd:pattern>
			<xsd:minLength value="1"></xsd:minLength>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CondDateTime">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\d{4}(((0[1-9])|(1[0-2]))(((0[1-9])|([1-2]\d)|(3[0-1]))((([01]\d|2[0-3])([0-5]\d))(([0-5]\d)((\.\d{1,4}))?)?)?)?)?([+\-](([0]\d|1[0-3])([0-5]\d)))?)?"></xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
