			<!-- Patient external identifier -->
			<xsd:enumeration value="QA"/>
			<!-- QA number -->
			<xsd:enumeration value="RRI"/>
			<!-- Regional Registry Number -->
			<xsd:enumeration value="SR"/>
			<!-- State Registry Number -->
			<xsd:enumeration value="TAG"/>
			<!-- Tag -->
			<xsd:enumeration value="TAGB"/>
			<!-- Tag, Back -->
			<xsd:enumeration value="TAGC"/>
			<!-- Tag, FSIS Condemnation  -->
			<xsd:enumeration value="TAGE"/>
			<!-- Tag, Ear -->
			<xsd:enumeration value="TAGF"/>
			<!-- Tag, Fin  -->
			<xsd:enumeration value="TAGN"/>
			<!-- Tag, Neck  -->
			<xsd:enumeration value="TAGOB "/>
			<!-- Tag, Official USDA Bangle  -->
			<xsd:enumeration value="TAGOE"/>
			<!-- Tag, Owner Ear -->
			<xsd:enumeration value="TAGOS"/>
			<!-- Tag, Official USDA Silver  -->
			<xsd:enumeration value="TAGOV"/>
			<!-- Tag, Official USDA Vaccination  -->
			<xsd:enumeration value="TAGT"/>
			<!-- Tag, Tail -->
